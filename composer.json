{
    "name": "osmbe/osm-welcome-tool",
    "description": "Website to help welcoming new OpenStreetMap contributors",
    "license": "ISC",
    "type": "project",
    "require": {
        "php": ">=8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-pdo": "*",
        "ext-sodium": "*",
        "composer/package-versions-deprecated": "^1.11.99.2",
        "doctrine/doctrine-bundle": "^2.4",
        "doctrine/doctrine-migrations-bundle": "^3.1",
        "doctrine/orm": "^2.9",
        "jbelien/oauth2-openstreetmap": "^0.1.1",
        "knplabs/knp-time-bundle": "^1.19",
        "knpuniversity/oauth2-client-bundle": "^2.8",
        "league/commonmark": "^2.0",
        "phpdocumentor/reflection-docblock": "^5.2",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "6.3.*",
        "symfony/cache": "6.3.*",
        "symfony/console": "6.3.*",
        "symfony/crowdin-translation-provider": "6.3.*",
        "symfony/dotenv": "6.3.*",
        "symfony/expression-language": "6.3.*",
        "symfony/flex": "^1.0 || ^2.0",
        "symfony/form": "6.3.*",
        "symfony/framework-bundle": "6.3.*",
        "symfony/http-client": "6.3.*",
        "symfony/intl": "6.3.*",
        "symfony/mailer": "6.3.*",
        "symfony/mime": "6.3.*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/notifier": "6.3.*",
        "symfony/process": "6.3.*",
        "symfony/property-access": "6.3.*",
        "symfony/property-info": "6.3.*",
        "symfony/proxy-manager-bridge": "6.3.*",
        "symfony/runtime": "6.3.*",
        "symfony/security-bundle": "6.3.*",
        "symfony/serializer": "6.3.*",
        "symfony/string": "6.3.*",
        "symfony/translation": "6.3.*",
        "symfony/twig-bundle": "6.3.*",
        "symfony/validator": "6.3.*",
        "symfony/web-link": "6.3.*",
        "symfony/webpack-encore-bundle": "^2.0",
        "symfony/yaml": "6.3.*",
        "twig/extra-bundle": "^3.3",
        "twig/html-extra": "^3.3",
        "twig/intl-extra": "^3.3",
        "twig/markdown-extra": "^3.3",
        "twig/twig": "^3.3"
    },
    "require-dev": {
        "deployer/deployer": "^7.0",
        "ergebnis/composer-normalize": "^2.15",
        "friendsofphp/php-cs-fixer": "^3.0",
        "friendsoftwig/twigcs": "^6.0",
        "phpro/grumphp": "^1.6",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^1.3.2",
        "phpstan/phpstan-strict-rules": "^1.1.0",
        "phpstan/phpstan-symfony": "^1.0.4",
        "phpunit/phpunit": "^10.1",
        "pluswerk/grumphp-xliff-task": "^5.0",
        "symfony/browser-kit": "^6.0",
        "symfony/css-selector": "^6.0",
        "symfony/debug-bundle": "^6.0",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^6.0",
        "symfony/stopwatch": "^6.0",
        "symfony/var-dumper": "^6.0",
        "symfony/web-profiler-bundle": "^6.0"
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "ergebnis/composer-normalize": true,
            "phpstan/extension-installer": true,
            "phpro/grumphp": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.3.*"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
